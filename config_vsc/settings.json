{

  "material-icon-theme.activeIconPack": "react_redux",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.labelFormat": "short",
  "editor.experimental.stickyScroll.enabled": true,
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "jspm_packages": true,
    "**/node_modules": true,
    "node_modules": true,
    ".sonar": true,
    "manifest.json": true,
    ".gitignore": false,
    ".editorconfig": false,
    "debug.log": true,
    "package-lock.json": true,
    "public": false
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "package-lock.json": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/node_modules": true,
    "public": true,
    "package-lock.json": true
  },
  "editor.fontFamily": "'Fira Code'",
  "editor.fontLigatures": true,
  "editor.fontSize": 17,
  "editor.formatOnPaste": true,
  "editor.insertSpaces": false,
  "breadcrumbs.enabled": true,
  "editor.rulers": [
    80,
    120,
    180
  ],
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.colorTheme": "One Dark Pro Flat",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "editor.bracketPairColorization.enabled": true,
  "powermode.enabled": true,
  "powermode.combo.location": "statusbar",
  "powermode.combo.counterEnabled": "default",
  "powermode.combo.timerEnabled": "hide",
  "powermode.shake.enabled": false,
  "editor.formatOnSave": true,

  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "stylelint.config": {
    "rules": {
      "at-rule-name-case": "lower",
      "at-rule-name-space-after": "always-single-line",
      "at-rule-semicolon-newline-after": "always"
    }
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.stylelint": true
  },

  "multiCommand.commands": [
    {
      "command": "multiCommand.consoleLogBasic",
      "sequence": [
        "editor.action.clipboardCopyAction",
        "editor.action.insertLineAfter",
        {
          "command": "editor.action.insertSnippet",
          "args": {
            "snippet": "console.log('${TM_FILENAME} --> ${CLIPBOARD} ==>', ${CLIPBOARD}$1)$2;"
          }
        }
      ]
    },
    {
      "command": "multiCommand.consoleLogTable",
      "sequence": [
        "editor.action.clipboardCopyAction",
        "editor.action.insertLineAfter",
        {
          "command": "editor.action.insertSnippet",
          "args": {
            "snippet": "console.table(${CLIPBOARD}$1)$2;"
          }
        }
      ]
    },
    {
      "command": "multiCommand.consoleLogColors",
      "sequence": [
        "editor.action.clipboardCopyAction",
        "editor.action.insertLineAfter",
        {
          "command": "editor.action.insertSnippet",
          "args": {
            "snippet": "console.log('%c${TM_FILENAME} --> ${CLIPBOARD} ==>', 'color:#FF8A71;font-size:1rem;font-weight:bold;', ${CLIPBOARD}$1)$2;"
          }
        }
      ]
    }
  ],
  "javascript.updateImportsOnFileMove.enabled": "always",
  "inlineFold.supportedLanguages": [
    "html",
    "svelte",
    "vue",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "rapidapi.terminalLink.enabled": false,

    "todo-tree.customHighlight": {

        "TODO": {
            "icon": "check",
            "type": "line"
        },

        "FIXME": {
            "icon"      : "flame",
            "foreground": "#FFF",
            "iconColour": "#ff4081"
        },

        "ALAN": {
            "icon"      : "flame",
            "foreground": "#FFF",
            "iconColour": "#9b59b6"
        },

        "ROGELIO": {
            "icon"      : "flame",
            "foreground": "#FFF",
            "iconColour": "#2e86c1"
        },

        "JAIR": {
            "icon"      : "flame",
            "foreground": "#FFF",
            "iconColour": "#34495E"
        },

        "FRAN": {
            "icon"      : "flame",
            "foreground": "#FFF",
            "iconColour": "#000000"
        },

        "DAMIAN": {
            "icon"      : "flame",
            "foreground": "#FFF",
            "iconColour": "#D35400"
        },
    },

    "todohighlight.keywords": [
        "DEBUG:",
        "REVIEW:",
        {
            "text": "NOTE:",
            "color": "#fff",
            "border": "1px",
            "borderRadius": "1px",
            "backgroundColor": "rgb(39, 174, 96)",
            "overviewRulerColor": "rgba(39, 174, 96,.2)"
        },

        {
            "text": "ALAN:",
            "color": "#fff",
            "border": "1px",
            "borderRadius": "1px",
            "backgroundColor": "rgb(155, 89, 182)",
            "overviewRulerColor": "rgba(155, 89, 182,.2)"
        },

        {
            "text": "ROGELIO:",
            "color": "#fff",
            "border": "1px",
            "borderRadius": "1px",
            "backgroundColor": "rgb(46, 134, 193)",
            "overviewRulerColor": "rgba(46, 134, 193,.2)"
        },

        {
            "text": "JAIR:",
            "color": "#fff",
            "border": "1px",
            "borderRadius": "1px",
            "backgroundColor": "rgb(52, 73, 94)",
            "overviewRulerColor": "rgba(52, 73, 94,.2)"
        },

        {
            "text": "FRAN:",
            "color": "#fff",
            "border": "1px",
            "borderRadius": "1px",
            "backgroundColor": "rgb(000, 000, 000)",
            "overviewRulerColor": "rgba(000, 000, 000,.2)"
        },

        {
            "text": "DAMIAN:",
            "color": "#fff",
            "border": "1px",
            "borderRadius": "1px",
            "backgroundColor": "rgb(211, 84, 0)",
            "overviewRulerColor": "rgba(211, 84, 0,.2)"
        }

        ]
}
