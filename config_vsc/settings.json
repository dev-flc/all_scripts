{
  "files.eol": "\n",
  "window.zoomLevel": 1.5,
  "editor.fontSize": 17,
  "editor.fontFamily": "'Fira Code'",
  "editor.rulers": [80, 120, 180],
  "editor.formatOnPaste": true,
  "editor.fontLigatures": true,
  "editor.cursorBlinking": "expand",
  "editor.guides.bracketPairs": true,
  "files.autoSave":"afterDelay",
  "workbench.iconTheme": "material-icon-theme",
  "breadcrumbs.enabled": false,
  "explorer.confirmDelete": false,
  "editor.linkedEditing": true,
  "editor.stickyScroll.enabled": true,
  "terminal.integrated.fontFamily": "'Fira Code'",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.guides.highlightActiveBracketPair": true,
  "security.workspace.trust.untrustedFiles": "open",
  "editor.bracketPairColorization.enabled": true,
  "material-icon-theme.activeIconPack": "react_redux",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "workbench.editor.labelFormat": "short",
  "editor.minimap.renderCharacters": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "material-icon-theme.folders.theme": "specific",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,

  "terminal.integrated.commandsToSkipShell" : [
    "psl.stepIn",
    "psl.stepOut",
    "psl.stepOver",
    "psl.sendToHostTerminal"
  ],

  "editor.language.colorizedBracketPairs" : [
    ["[", "]"],
    ["(", ")"],
    ["{", "}"],
    ["do", "done"],
    ["if", "elif", "else", "fi"],
    ["case", "esac"]
  ],

  "[python]" : {
    "editor.language.colorizedBracketPairs" : [[":"]],
    "editor.formatOnType"                   : true
  },

  "[shellscript]" : {
    "editor.language.colorizedBracketPairs": [
      ["[", "]"],
      ["{", "}"],
      ["(", ")"],
      ["do", "done"],
      ["if", "elif", "else", "fi"],
      ["case", "esac"]
    ]
  },

  "workbench.colorCustomizations": {
    "editorBracketHighlight.foreground1" : "#fc9867",
    "editorBracketHighlight.foreground2" : "#ff6188",
    "editorBracketHighlight.foreground3" : "#ffd866",
    "editorBracketHighlight.foreground4" : "#a9dc76",
    "editorBracketHighlight.foreground5" : "#78dce8",
    "editorBracketHighlight.foreground6" : "#ff6188",

    "editorBracketPairGuide.background1" : "#fc9867",
    "editorBracketPairGuide.background2" : "#ff6188",
    "editorBracketPairGuide.background3" : "#ffd866",
    "editorBracketPairGuide.background4" : "#a9dc76",
    "editorBracketPairGuide.background5" : "#78dce8",
    "editorBracketPairGuide.background6" : "#ff6188",

    "editorBracketPairGuide.activeBackground1" : "#fc9867",
    "editorBracketPairGuide.activeBackground2" : "#ff6188",
    "editorBracketPairGuide.activeBackground3" : "#ffd866",
    "editorBracketPairGuide.activeBackground4" : "#a9dc76",
    "editorBracketPairGuide.activeBackground5" : "#78dce8",
    "editorBracketPairGuide.activeBackground6" : "#ff6188",

    "editorGroupHeader.tabsBackground" : "#1F262E",
    "editorGroupHeader.border"         : "#303740",

    "tab.inactiveBackground" : "#1F262E",
    "tab.activeBackground"   : "#303740",
    "tab.activeBorder"       : "#303740",
    "tab.activeForeground"   : "#c0cada",
    "tab.hoverBackground"    : "#303740",

    "commandCenter.border"             : "#303740",
    "commandCenter.activeBackground"   : "#1F262E",
    "commandCenter.activeForeground"   : "#c0cada",
    "commandCenter.inactiveBackground" : "#1F262E",
    "commandCenter.inactiveForeground" : "#c0cada",

    "dropdown.border"         : "#303740",
    "dropdown.foreground"     : "#c0cada",
    "dropdown.background"     : "#1F262E",
    "dropdown.listBackground" : "#1F262E",

    "input.border"     : "#303740",
    "input.background" : "#1F262E",
    "input.foreground" : "#c0cada",

    "menu.foreground" : "#c0cada",
    "menu.background" : "#1F262E",
    "menu.border"     : "#303740",

    "titleBar.inactiveBackground" : "#1F262E",
    "titleBar.inactiveForeground" : "#c0cada",
    "titleBar.activeBackground"   : "#1F262E",
    "titleBar.activeForeground"   : "#c0cada",
    "titleBar.border"             : "#303740",

    "panel.border"                  : "#303740",
    "panel.background"              : "#1F262E",
    "panel.dropBorder"              : "#303740",
    "panelInput.border"             : "#303740",
    "panelSection.border"           : "#303740",
    "panelTitle.activeBorder"       : "#c0cada",
    "panelTitle.activeForeground"   : "#c0cada",
    "panelTitle.inactiveForeground" : "#484a4e",

    "editorWidget.background" : "#1F262E",
    "editorWidget.foreground" : "#c0cada",
    "editorWidget.border"     : "#303740",

    "editor.background"                 : "#1F262E",
    "editor.foreground"                 : "#c0cada",
    "editorGroup.border"                : "#303740",
    "editorRuler.foreground"            : "#303740",
    "editorLineNumber.activeForeground" : "#c0cada",

    "button.background"      : "#434D5B",
    "button.hoverBackground" : "#303740",

    "focusBorder"     : "#484a4e",
    "icon.foreground" : "#c0cada",

    "activityBar.foreground" : "#c0cada",
    "activityBar.background" : "#1F262E",
    "activityBar.border"     : "#303740",

    "settings.numberInputBackground"  : "#1F262E",
    "settings.numberInputForeground"  : "#c0cada",
    "settings.numberInputBorder"      : "#303740",
    "settings.textInputBackground"    : "#1F262E",
    "settings.textInputForeground"    : "#c0cada",
    "settings.textInputBorder"        : "#303740",
    "settings.checkboxForeground"     : "#c0cada",
    "settings.checkboxBackground"     : "#1F262E",
    "settings.checkboxBorder"         : "#303740",
    "settings.dropdownForeground"     : "#c0cada",
    "settings.dropdownBackground"     : "#1F262E",
    "settings.dropdownBorder"         : "#303740",
    "settings.dropdownListBorder"     : "#1F262E",
    "settings.rowHoverBackground"     : "#303740",

    "keybindingLabel.foreground" : "#c0cada",
    "keybindingLabel.background" : "#ff6188",

    "list.activeSelectionBackground" : "#1F262E",
    "list.activeSelectionForeground" : "#c0cada",

    "sideBar.foreground" : "#c0cada",
    "sideBar.background" : "#1F262E",
    "sideBar.border"     : "#303740",

    "foreground"           : "#c0cada",
    "statusBar.background" : "#1F262E",
    "terminal.background"  : "#1F262E",


    "badge.background" : "#484a4e",
    "badge.foreground" : "#c0cada",

    "sideBarSectionHeader.background": "#303740",
  },

  "files.exclude": {
    "**/.git"           : true,
    "**/node_modules"   : true,
    "node_modules"      : true,
    ".sonar"            : true,
    "manifest.json"     : true,
    ".gitignore"        : true,
    ".editorconfig"     : true,
    "debug.log"         : true,
    "package-lock.json" : true,
    "public"            : true,
  },

  "search.exclude": {
    "**/.git"           : true,
    "**/node_modules"   : true,
    "node_modules"      : true,
    ".sonar"            : true,
    "manifest.json"     : true,
    ".gitignore"        : true,
    ".editorconfig"     : true,
    "debug.log"         : true,
    "package-lock.json" : true,
    "public"            : true,
  },

  "files.watcherExclude": {
    "**/.git"           : true,
    "**/node_modules"   : true,
    "node_modules"      : true,
    ".sonar"            : true,
    "manifest.json"     : true,
    ".gitignore"        : true,
    ".editorconfig"     : true,
    "debug.log"         : true,
    "package-lock.json" : true,
    "public"            : true,
  },

  "multiCommand.commands": [
    {
      "command"  : "multiCommand.consoleLogBasic",
      "sequence" : [
        "editor.action.clipboardCopyAction",
        "editor.action.insertLineAfter",
        {
          "command" : "editor.action.insertSnippet",
          "args"    : {
            "snippet" : "console.log('${CLIPBOARD}==>', ${CLIPBOARD}$1)$2;"
          }
        }
      ]
    },
    {
      "command"  : "multiCommand.consoleLogTable",
      "sequence" : [
        "editor.action.clipboardCopyAction",
        "editor.action.insertLineAfter",
        {
          "command" : "editor.action.insertSnippet",
          "args"    : {
            "snippet" : "console.table(${CLIPBOARD}$1)$2;"
          }
        }
      ]
    },
    {
      "command"  : "multiCommand.consoleLogColors",
      "sequence" : [
        "editor.action.clipboardCopyAction",
        "editor.action.insertLineAfter",
        {
          "command" : "editor.action.insertSnippet",
          "args"    : {
            "snippet" : "console.log('%c${CLIPBOARD}==>', 'color:#ff6188;font-size:1rem;font-weight:bold;', ${CLIPBOARD}$1)$2;"
          }
        }
      ]
    }
  ],
  "editor.insertSpaces": false
}
