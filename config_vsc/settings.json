{
  "files.eol": "\n",
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.fontSize": 17,
  "editor.formatOnPaste": true,
  "editor.insertSpaces": false,
  "breadcrumbs.enabled": false,
  "editor.rulers": [80, 120, 180],
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.colorTheme": "One Dark Pro Flat",
  "editor.bracketPairColorization.enabled": true,
  "material-icon-theme.activeIconPack": "react_redux",
  "workbench.editor.labelFormat": "short",
  "editor.minimap.renderCharacters": false,
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "on",
  "terminal.integrated.env.linux": {},
  "terminal.integrated.env.windows": {},
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "console-ninja.showWhatsNew": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "cSpell.language": "en,es",
  "material-icon-theme.folders.theme": "specific",
  "workbench.iconTheme": "material-icon-theme",
  "console-ninja.featureSet": "Community",
  "editor.linkedEditing": true,
  "editor.stickyScroll.enabled": true,
  "explorer.confirmDelete": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.guides.bracketPairs": true,

  "terminal.integrated.commandsToSkipShell": [
    "psl.stepIn",
    "psl.stepOut",
    "psl.stepOver",
    "psl.sendToHostTerminal"
  ],

  "editor.language.colorizedBracketPairs": [
    ["[", "]"],
    ["(", ")"],
    ["{", "}"],
    ["do", "done"],
    ["if", "elif", "else", "fi"],
    ["case", "esac"]
  ],

  "[python]": {
    "editor.language.colorizedBracketPairs": [[":"]],
    "editor.formatOnType": true
  },

  "[shellscript]": {
    "editor.language.colorizedBracketPairs": [
      ["[", "]"],
      ["{", "}"],
      ["(", ")"],
      ["do", "done"],
      ["if", "elif", "else", "fi"],
      ["case", "esac"]
    ]
  },

  "workbench.colorCustomizations": {
    "editorBracketHighlight.foreground1": "#fc9867",
    "editorBracketHighlight.foreground2": "#ff6188",
    "editorBracketHighlight.foreground3": "#ffd866",
    "editorBracketHighlight.foreground4": "#a9dc76",
    "editorBracketHighlight.foreground5": "#78dce8",
    "editorBracketHighlight.foreground6": "#ff6188",

    "editorBracketPairGuide.background1": "#fc9867",
    "editorBracketPairGuide.background2": "#ff6188",
    "editorBracketPairGuide.background3": "#ffd866",
    "editorBracketPairGuide.background4": "#a9dc76",
    "editorBracketPairGuide.background5": "#78dce8",
    "editorBracketPairGuide.background6": "#ff6188",

    "editorBracketPairGuide.activeBackground1": "#fc9867",
    "editorBracketPairGuide.activeBackground2": "#ff6188",
    "editorBracketPairGuide.activeBackground3": "#ffd866",
    "editorBracketPairGuide.activeBackground4": "#a9dc76",
    "editorBracketPairGuide.activeBackground5": "#78dce8",
    "editorBracketPairGuide.activeBackground6": "#ff6188",

    "editor.background"                 : "#1F262E",
    "editor.border"                     : "#ff6188",
    "button.background"                 : "#434D5B",
    "button.hoverBackground"            : "#303740",
    "editorRuler.foreground"            : "#ff6188",
    "tab.activeBorder"                  : "#ff6188",
    "focusBorder"                       : "#484a4e",
    "icon.foreground"                   : "#484a4e",
    "activityBar.foreground"            : "#c0cada",
    "activityBar.background"            : "#1F262E",
    "activityBar.border"                : "#797979",
    "sideBar.foreground"                : "#c0cada",
    "sideBar.background"                : "#1F262E",
    "sideBar.border"                    : "#797979",
    "foreground"                        : "#c0cada",
    "statusBar.background"              : "#1F262E",
    "terminal.background"               : "#1F262E",
    "badge.background"                  : "#484a4e",
    "badge.foreground"                  : "#c0cada",
    "sideBarSectionHeader.background"   :"#303740",
    "editorLineNumber.activeForeground" : "#78dce8",
    "editorGroup.border"                : "#797979",
  },

  "files.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "node_modules": true,
    ".sonar": true,
    "manifest.json": true,
    ".gitignore": true,
    ".editorconfig": true,
    "debug.log": true,
    "package-lock.json": true,
    "public": true,
  },

  "search.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "node_modules": true,
    ".sonar": true,
    "manifest.json": true,
    ".gitignore": true,
    ".editorconfig": true,
    "debug.log": true,
    "package-lock.json": true,
    "public": true,
  },

  "files.watcherExclude": {
     "**/.git": true,
    "**/node_modules": true,
    "node_modules": true,
    ".sonar": true,
    "manifest.json": true,
    ".gitignore": true,
    ".editorconfig": true,
    "debug.log": true,
    "package-lock.json": true,
    "public": true,
  },

  "multiCommand.commands": [
    {
      "command": "multiCommand.consoleLogBasic",
      "sequence": [
        "editor.action.clipboardCopyAction",
        "editor.action.insertLineAfter",
        {
          "command": "editor.action.insertSnippet",
          "args": {
            "snippet": "console.log('${CLIPBOARD}==>', ${CLIPBOARD}$1)$2;"
          }
        }
      ]
    },
    {
      "command": "multiCommand.consoleLogTable",
      "sequence": [
        "editor.action.clipboardCopyAction",
        "editor.action.insertLineAfter",
        {
          "command": "editor.action.insertSnippet",
          "args": {
            "snippet": "console.table(${CLIPBOARD}$1)$2;"
          }
        }
      ]
    },
    {
      "command": "multiCommand.consoleLogColors",
      "sequence": [
        "editor.action.clipboardCopyAction",
        "editor.action.insertLineAfter",
        {
          "command": "editor.action.insertSnippet",
          "args": {
            "snippet": "console.log('%c${CLIPBOARD}==>', 'color:#ff6188;font-size:1rem;font-weight:bold;', ${CLIPBOARD}$1)$2;"
          }
        }
      ]
    }
  ]
}